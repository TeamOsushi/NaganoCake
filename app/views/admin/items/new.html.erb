<h1>商品新規作成</h1>

<%= form_with model:@item, local:true do |f| %>
 <h4>商品の名前</h4>
 <%= f.text_field :item_name %>
 <h4>商品のジャンル</h4>
 <%= f.text_field :genre %>
 <h4>商品の画像</h4>
 <%= f.attachment_field :image %>
 <h4>商品の説明文</h4>
 <%= f.text_area :introduction %>
 <h4>商品の販売状況</h4>
 <%= f.text_field :is_sales_status %>
 <h4>商品の値段</h4>
 <%= f.text_field :price_without_tax %>
 <%= f.submit '商品を追加' %>
<% end %>