<div class="row">
<h2 class="genre-edit-head">ジャンル編集</h2>
<%= form_with(model: @genre, url:admin_genre_path, local: true ,method: :patch) do |f| %>
	<%= f.text_field :name %>
	<label>
	<%= f.radio_button :is_genre_status, :true %>有効
	</label>
	<label>
	<%= f.radio_button :is_genre_status, :false %>無効
	</label>
	<%= f.submit '変更を保存する',class:"btn btn-success genre-submit" %>
<% end %>
<div class="col-xs-12">
    <%= link_to "戻る", admin_genres_path, class: "glyphicon glyphicon-home btn btn-info" %>
  </div>